\u064a\u0631 \u0628\u0627\u0644\u063a\u0645\u0627\u0632 \u0627\u0644\u0623\u064a\u0633\u0631 \u0648\u0642\u062a \u0637\u0648\u064a\u0644 :","a":"\u064a\u062c\u0628 \u062a\u0646\u0628\u064a\u0647 \u0633\u0627\u0626\u0642 \u0627\u0644\u0645\u0631\u0643\u0628\u0629 \u0627\u0644\u062a\u064a \u0623\u0645\u0627\u0645\u0646\u0627 \u0628\u0648\u0627\u0633\u0637\u0629 \u0627\u0644\u0632\u0627\u0645\u0648\u0631 \u0623\u0648\u0644\u0627\u064b \u062b\u0645 \u0646\u062c\u062a\u0627\u0632\u0647\u0627.","b":"\u064a\u062c\u0648\u0632 \u0639\u0628\u0648\u0631 \u0627\u0644\u0645\u0631\u0643\u0628\u0629 \u0645\u0646 \u062c\u0647\u062a\u0647\u0627 \u0627\u0644\u064a\u0645\u0646\u0649.","c":"\u0645\u0645\u0646\u0648\u0639 \u0627\u062c\u062a\u064a\u0627\u0632\u0647\u0627 \u0644\u0639\u0644\u0651 \u0633\u0627\u0626\u0642\u0647\u0627 \u064a\u0646\u0639\u0637\u0641 \u0625\u0644\u0649 \u0627\u0644\u064a\u0633\u0627\u0631.","d":"\u064a\u062c\u0648\u0632 \u0627\u062c\u062a\u064a\u0627\u0632\u0647\u0627 \u0628\u062d\u0630\u0631 \u0644\u0623\u0646 \u0633\u0627\u0626\u0642 \u0647\u0630\u0647 \u0627\u0644\u0645\u0631\u0643\u0628\u0629 \u0642\u062f \u0646\u0633\u064a \u0627\u0644\u063a\u0645\u0627\u0632.","answer":"3"},{"id":1903,"question":"\u0625\u0630\u0627 \u062d\u062f\u062b \u0628\u0646\u0634\u0631 \u0641\u064a \u0627\u0644\u0639\u062c\u0644 :","a":"\u064a\u062c\u0648\u0632 \u0627\u0644\u0633\u0631 \u0644\u0645\u0633\u0627\u0641\u0629 \u0643\u064a\u0644\u0648\u0645\u062a\u0631 \u0648\u0627\u062d\u062f \u0641\u0642\u0637.","b":"\u0645\u0645\u0646\u0648\u0639 \u0627\u0644\u0633\u064a\u0631 \u0639\u0644\u0649 \u0647\u0630\u0627 \u0627\u0644\u0639\u062c\u0644.","c":"\u064a\u062c\u0648\u0632 \u0627\u0644\u0633\u064a\u0631 \u0628\u0628\u0637\u0621 \u0625\u0644\u0649 \u0623\u0642\u0631\u0628 \u0645\u0643\u0627\u0646 \u0644\u062a\u0635\u0644\u064a\u062d \u0627\u0644\u0628\u0646\u0627\u0634\u0631.","d":"\u064a\u062c\u0648\u0632 \u0627\u0644\u0633\u064a\u0631 \u0628\u0628\u0637\u0621 \u0625\u0644\u0649 \u0623\u0642\u0631\u0628 \u0645\u062d\u0637\u0629 \u0648\u0642\u0648\u062f.","answer":"2"},{"id":1904,"question":"\u062a\u062a\u0648\u0642\u0641 \u0645\u0633\u0627\u0641\u0629 \u0627\u0644\u0648\u0642\u0648\u0641 \u0639\u0644\u0649 :","a":"\u062c\u0645\u064a\u0639 \u0627\u0644\u0625\u062c\u0627\u0628\u0627\u062a \u0635\u062d\u064a\u062d\u0629.","b":"\u062d\u0627\u0644\u0629 \u0627\u0644\u0625\u0637\u0627\u0631\u0627\u062a \u0648\u0627\u0644\u0641\u0631\u0627\u0645\u0644.","c":"\u062d\u0645\u0648\u0644\u0629 \u0627\u0644\u0645\u0631\u0643\u0628\u0629.","d":"\u062d\u0627\u0644\u0629 \u0627\u0644\u0637\u0631\u064a\u0642.","answer":"1"},{"id":1905,"question":"\u0645\u0633\u0627\u0641\u0629 \u0631\u062f \u0627\u0644\u0641\u0639\u0644 \u0647\u064a \u0627\u0644\u0645\u0633\u0627\u0641\u0629 \u0627\u0644\u062a\u064a \u062a\u0642\u0637\u0639\u0647\u0627 \u0627\u0644\u0645\u0631\u0643\u0628\u0629 :","a":"\u0645\u0646 \u0644\u062d\u0638\u0629 \u0631\u0641\u0639 \u0627\u0644\u0631\u062c\u0644 \u0639\u0646 \u062f\u0639\u0633\u0629 \u0627\u0644\u0648\u0642\u0648\u062f \u0644\u063a\u0627\u064a\u0629 \u0627\u0644\u0648\u0642\u0648\u0641 \u0627\u0644\u062a\u0627\u0645.","b":"\u0645\u0646 \u0644\u062d\u0638\u0629 \u0631\u0624\u064a\u0629 \u0627\u0644\u062e\u0637\u0631 \u0644\u063a\u0627\u064a\u0629 \u0627\u0644\u0636\u063a\u0637 \u0639\u0644\u0649 \u062f\u0639\u0633\u0629 \u0627\u0644\u0641\u0631\u0627\u0645\u0644.","c":"\u0645\u0646 \u0644\u062d\u0638\u0629 \u0627\u0644\u0636\u063a\u0637 \u0639\u0644\u0649 \u062f\u0639\u0633\u0629 \u0627\u0644\u0641\u0631\u0627\u0645\u0644 \u0644\u063a\u0627\u064a\u0629 \u0627\u0644\u0648\u0642\u0648\u0641 \u0627\u0644\u062a\u0627\u0645.","d":"\u0645\u0646 \u0644\u062d\u0638\u0629 \u0631\u0624\u064a\u0629 \u0627\u0644\u062e\u0637\u0631 \u0644\u063a\u0627\u064a\u0629 \u0627\u0644\u0648\u0642\u0648\u0641 \u0627\u0644\u062a\u0627\u0645.","answer":"2"},{"id":1906,"question":"\u062e\u0637\u0631 \u0627\u0644\u062a\u0632\u062d\u0644\u0642 \u0639\u0644\u0649 \u0637\u0631\u064a\u0642 \u0631\u0637\u0628\u0629 \u064a\u0643\u0648\u0646 \u0623\u0634\u062f :","a":"\u0643\u0644\u0645\u0627 \u0643\u0627\u0646\u062a \u0641\u0631\u0632\u0627\u062a \u0627\u0644\u0625\u0637\u0627\u0631\u0627\u062a \u0623\u0639\u0645\u0642.","b":"\u0639\u0646\u062f\u0645\u0627 \u064a\u0643\u0648\u0646 \u0642\u064a\u0627\u0633 \u0627\u0644\u0625\u0637\u0627\u0631\u0627\u062a \u063a\u064a\u0631 \u0645\u0644\u0627\u0626\u0645 \u0644\u0644\u0645\u0631\u0643\u0628\u0629.","c":"\u0643\u0644\u0645\u0627 \u0643\u0627\u0646\u062a \u0627\u0644\u0625\u0637\u0627\u0631\u0627\u062a \u063a\u064a\u0631 \u0635\u0627\u0644\u062d\u0629 \u0648\u0645\u0645\u0633\u0648\u062d\u0629.","d":"\u0644\u0627 \u0639\u0644\u0627\u0642\u0629 \u0628\u064a\u0646 \u062d\u0627\u0644\u0629 \u0627\u0644\u0625\u0637\u0627\u0631\u0627\u062a \u0648\u0627\u0644\u062a\u0632\u062d\u0644\u0642.","answer":"3"},{"id":1907,"question":"\u0639\u0646\u062f \u0648\u062c\u0648\u062f \u062d\u0641\u0631 \u0648\u0641\u062c\u0648\u0627\u062a \u0639\u0644\u0649 \u0627\u0644\u0637\u0631\u064a\u0642 :","a":"\u0627\u0644\u0632\u0646\u0628\u0631\u0643\u0627\u062a \u0641\u064a \u0627\u0644\u0645\u0631\u0643\u0628\u0629 \u062a\u0645\u0643\u0646 \u0645\u0646 \u0633\u064a\u0631 \u0627\u0639\u062a\u064a\u0627\u062f\u064a \u0641\u064a \u0637\u0631\u064a\u0642 \u0648\u0639\u0631\u0629 \u0623\u064a\u0636\u0627\u064b.","b":"\u062a\u062a\u0645\u0647\u0644 \u0625\u0644\u0649 \u062d\u062f \u0625\u0645\u0643\u0627\u0646\u064a\u0629 \u0627\u0644\u0648\u0642\u0648\u0641 \u0639\u0646\u062f \u0627\u0644\u0627\u0642\u062a\u0631\u0627\u0628 \u0645\u0646 \u0627\u0644\u062d\u0641\u0631 \u0648\u0627\u0644\u0641\u062c\u0648\u0627\u062a.","c":"\u0644\u0627 \u062a\u0632\u064a\u062f \u0645\u0646 \u0633\u0631\u0639\u062a\u0643 \u0648\u0623\u0639\u0628\u0631 \u0627\u0644\u062d\u0641\u0631 \u0648\u0627\u0644\u0641\u062c\u0648\u0627\u062a.","d":"\u062a\u0642\u0641 \u0642\u0628\u0644 \u0643\u0644 \u062d\u0641\u0631\u0629 \u0644\u0643\u064a \u062a\u0639\u0628\u0631\u0647\u0627.","answer":"2"},{"id":1908,"question":"\u0645\u0627\u0630\u0627 \u064a\u0633\u0628\u0628 \u0631\u0645\u0644 \u0639\u0644\u0649 \u0633\u0637\u062d \u0627\u0644\u0637\u0631\u064a\u0642 \u061f","a":"\u0644\u0627 \u064a\u0648\u062c\u062f \u0623\u064a \u062a\u0623\u062b\u064a\u0631 \u0639\u0644\u0649 \u0627\u0644\u0645\u0631\u0643\u0628\u0629 \u0639\u0646\u062f \u0648\u062c\u0648\u062f \u0631\u0645\u0644 \u0639\u0644\u0649 \u0633\u0637\u062d \u0627\u0644\u0637\u0631\u064a\u0642.","b":"\u064a\u0633\u0628\u0628 \u0641\u064a \u0632\u064a\u0627\u062f\u0629 \u0627\u0644\u0627\u062d\u062a\u0643\u0627\u0643 \u0628\u064a\u0646 \u0627\u0644\u0625\u0637\u0627\u0631\u0627\u062a \u0648\u0633\u0637\u062d \u0627\u0644\u0637\u0631\u064a\u0642.","c":"\u064a\u062e\u0641\u0641 \u0645\u0646 \u062a\u0632\u062d\u0644\u0642 \u0627\u0644\u0645\u0631\u0643\u0628\u0629 \u0639\u0644\u0649 \u0627\u0644\u0637\u0631\u064a\u0642.","d":"\u0627\u062d\u062a\u0643\u0627\u0643 \u0642\u0644\u064a\u0644 \u0628\u064a\u0646 \u0627\u0644\u0625\u0637\u0627\u0631\u0627\u062a \u0648\u0633\u0637\u062d \u0627\u0644\u0637\u0631\u064a\u0642 \u0648\u0628\u0627\u0644\u062a\u0627\u0644\u064a \u062a\u0632\u062d\u0644\u0642 \u0627\u0644\u0645\u0631\u0643\u0628\u0629.","answer":"4"}]