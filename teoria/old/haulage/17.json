[{"id":780,"question":"\u0647\u0644 \u064a\u062c\u0648\u0632 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u0627\u0644\u0632\u0627\u0645\u0648\u0631 \u062f\u0627\u062e\u0644 \u0627\u0644\u0645\u062f\u0646 \u061f","a":"\u064a\u062c\u0648\u0632 \u0644\u0627\u0646\u0648\u0627\u0639 \u0645\u0639\u064a\u0646\u0629 \u0645\u0646 \u0627\u0644\u0645\u0631\u0643\u0628\u0627\u062a","b":"\u064a\u062c\u0648\u0632 \u0648\u0644\u0643\u0646 \u0644\u064a\u0633 \u0628\u0635\u0648\u0631\u0629 \u0645\u062a\u0648\u0627\u0635\u0644\u0629 \u0627\u0648 \u0645\u062a\u0643\u0631\u0631\u0629","c":"\u064a\u062c\u0648\u0632 \u0639\u0646\u062f \u0627\u0644\u062a\u062c\u0627\u0648\u0632 \u0641\u0642\u0637","d":"\u064a\u062c\u0648\u0632 \u0644\u062a\u062c\u0646\u0628 \u062e\u0637\u0631 \u0645\u062d\u062f\u0642 \u0644\u0627 \u064a\u0645\u0643\u0646 \u062a\u062c\u0646\u0628\u0647 \u0628\u0637\u0631\u064a\u0642\u0629 \u0627\u062e\u0631\u0649","answer":"4"},{"id":781,"question":"\u0641\u064a \u0637\u0631\u064a\u0642 \u0644\u064a\u0633\u062a \u0628\u0644\u062f\u064a\u0629 \u064a\u062c\u0648\u0632 \u0627\u0644\u0648\u0642\u0648\u0641 \u0639\u0644\u0649 \u0637\u0631\u0641 \u0627\u0644\u0637\u0631\u064a\u0642","a":"\u0644\u0627\u0646\u0632\u0627\u0644 \u0648\u0627\u0635\u0639\u0627\u062f \u0627\u0644\u0631\u0643\u0627\u0628 \u0645\u0627 \u0639\u062f\u0627 \u0627\u0644\u0637\u0631\u064a\u0642 \u0627\u0644\u0633\u0631\u064a\u0639\u0629","b":"\u064a\u062c\u0648\u0632 \u0644\u0644\u0645\u0631\u0643\u0628\u0627\u062a \u0627\u0644\u0639\u0645\u0648\u0645\u064a\u0629 \u0641\u0642\u0637","c":"\u064a\u062c\u0648\u0632 \u0644\u062c\u0645\u064a\u0639 \u0627\u0646\u0648\u0627\u0639 \u0627\u0644\u0645\u0631\u0643\u0628\u0627\u062a","d":"\u0645\u0645\u0646\u0648\u0639 \u0628\u062a\u0627\u062a\u0627","answer":"1"},{"id":782,"question":"\u062a\u0646\u0641\u0630 \u0627\u0644\u0627\u0633\u062a\u062f\u0627\u0631\u0629 \u0627\u0644\u0649 \u0627\u0644\u064a\u0645\u064a\u0646","a":"\u0645\u0646 \u0627\u0642\u0635\u0649 \u0627\u0