\u0631\u0643 \u0648\u062a\u0643\u0641\u064a \u0644\u0639\u0645\u0644 \u0627\u0644\u0645\u062d\u0631\u0643","c":"\u0646\u0639\u0645 \u060c \u064a\u062c\u0628 \u0627\u0646 \u062a\u0643\u0648\u0646 \u0643\u0645\u064a\u0629 \u0627\u0644\u0632\u064a\u062a \u0627\u0639\u0644\u0649 \u0628\u0642\u0644\u064a\u0644 \u0645\u0646 \u0627\u0644\u062d\u062f \u0644\u0623\u0646 \u0627\u0644\u0632\u064a\u062a \u064a\u062a\u0628\u062e\u0631 \u0645\u0639 \u0627\u0644\u062d\u0631\u0627\u0631\u0629","d":"\u0644\u0627 \u062a\u0648\u062c\u062f \u0627\u0647\u0645\u064a\u0629 \u0644\u0643\u0645\u064a\u0629 \u0627\u0644\u0632\u064a\u062a \u0627\u0630\u0627 \u0643\u0627\u0646\u062a \u0627\u0643\u062b\u0631 \u0627\u0648 \u0627\u0642\u0644 \u0645\u0646 \u0627\u0644\u062d\u062f \u0627\u0644\u0645\u0637\u0644\u0648\u0628 \u0627\u0644\u0645\u0647\u0645 \u0627\u0646 \u064a\u0643\u0648\u0646 \u0647\u0646\u0627\u0643 \u0636\u063a\u0637 \u0632\u064a\u062a","answer":"2"},{"id":1358,"question":"\u0647\u0644 \u064a\u062c\u0628 \u0623\u0646 \u0646\u0642\u0641\u0644 \u0645\u0636\u062e\u0629 \u0627\u0644\u0628\u062e\u0627\u062e\u0627\u062a \u0628\u0633\u062f\u0627\u062f \u0628\u0644\u0648\u0645\u0628\u0647 ( \u062e\u062a\u0645 \u0631\u0635\u0627\u0635\u064a )","a":"\u0646\u0639\u0645 \u0648\u0627\u062c\u0628 \u0641\u064a \u0645\u062d\u0631\u0643 \u0627\u0644\u0628\u0646\u0632\u064a\u0646","b":"\u0646\u0639\u0645 \u0648\u0627\u062c\u0628 \u0627\u0630\u0627 \u0643\u0627\u0646 \u0627\u0644\u0645\u0627\u062a\u0648\u0631 \u0623\u0643\u062b\u0631 \u0645\u0646 3 \u0633\u0644\u0646\u062f\u0631","c":"\u0644\u0627 \u064a\u0644\u0632\u0645 \u062a\u0631\u0643\u064a\u0628 \u0642\u0641\u0644 \u0628\u0644\u0648\u0645\u0628\u0647 ( \u062e\u062a\u0645 \u0631\u0635\u0627\u0635\u064a )\u0639\u0644\u0649 \u0645\u0636\u062e\u0629 \u0627\u0644\u0628\u062e\u0627\u062e","d":"\u0646\u0639\u0645 \u0648\u0627\u062c\u0628 \u062a\u0631\u0643\u064a\u0628 \u0642\u0641\u0644 \u0628\u0644\u0648\u0645\u0628\u0647 ( \u062e\u062a\u0645 \u0631\u0635\u0627\u0635\u064a )\u0639\u0644\u0649 \u0643\u0644 \u0645\u062d\u0631\u0643 \u062f\u064a\u0632\u0644","answer":"4"},{"id":1359,"question":"\u0647\u0644 \u064a\u062c\u0647\u0632 \u0627\u0644\u062a\u0631\u0627\u0643\u062a\u0648\u0631 \u0627\u0644\u0630\u064a \u064a\u0633\u064a\u0631 \u0639\u0644\u0649 \u0639\u062c\u0644\u0627\u062a \u0628\u0645\u0627\u0646\u0639 \u0627\u0647\u062a\u0632\u0627\u0632\u0627\u062a","a":" \u0644\u0627","b":"\u0646\u0639\u0645 \u060c \u0639\u0644\u0649 \u0627\u0644\u0645\u062d\u0648\u0631\u064a\u0646 \u0627\u0644\u0627\u0645\u0627\u0645\u064a \u0648\u0627\u0644\u062e\u0644\u0641\u064a","c":"\u0646\u063a\u0645 \u060c \u0639\u0644\u0649 \u0627\u0644\u0645\u062d\u0648\u0631 \u0627\u0644\u062e\u0644\u0641\u064a \u0641\u0642\u0637","d":"\u0646\u0639\u0645 \u060c \u0639\u0644\u0649 \u0627\u0644\u0645\u062d\u0648\u0631 \u0627\u0644\u0627\u0645\u0627\u0645\u064a \u0641\u0642\u0637","answer":"1"},{"id":1360,"question":"\u0647\u0644 \u064a\u062c\u0647\u0632 \u0627\u0644\u062a\u0631\u0627\u0643\u062a\u0648\u0631 \u0627\u0644\u0630\u064a \u064a\u0633\u064a\u0631 \u0639\u0644\u0649 \u0639\u062c\u0644\u0627\u062a \u0628\u0645\u0645\u062a\u0635 \u0627\u0644\u0635\u062f\u0645\u0627\u062a \u061f","a":"\u0643\u0644\u0627","b":"\u0646\u0639\u0645 \u060c \u0639\u0644\u0649 \u0627\u0644\u0645\u062d\u0648\u0631 \u0627\u0644\u0627\u0645\u0627\u0645\u064a \u0641\u0642\u0637","c":"\u0646\u0639\u0645 \u060c \u0639\u0644\u0649 \u0627\u0644\u0645\u062d\u0648\u0631\u064a\u064a\u0646 \u0627\u0644\u0627\u0645\u0627\u0645\u064a \u0648\u0627\u0644\u062e\u0644\u0641\u064a","d":"\u0646\u0639\u0645 \u060c \u0639\u0644\u0649 \u0627\u0644\u0645\u062d\u0648\u0631 \u0627\u0644\u062e\u0644\u0641\u064a \u0641\u0642\u0637","answer":"1"},{"id":1361,"question":"\u064a\u062c\u0628 \u062a\u0639\u064a\u064a\u0631 \u062a\u0648\u0627\u0632\u0646 \u0639\u062c\u0644\u0627\u062a \u0627\u0644\u0645\u0631\u0643\u0628\u0629","a":"\u0644\u0645\u0646\u0639 \u0643\u0633\u0631 \u0628\u0631\u0627\u063a\u064a \u0627\u0644\u062c\u0646\u0637","b":"\u0641\u0642\u0637 \u0644\u062a\u0633\u0647\u064a\u0644 \u0633\u064a\u0631 \u0627\u0644\u0645\u0631\u0643\u0628\u0629 \u0641\u064a \u0627\u0644\u0645\u0646\u0639\u0637\u0641\u0627\u062a","c":"\u0644\u062a\u0633\u0647\u064a\u0644 \u0639\u0645\u0644\u064a\u0629 \u0627\u0644\u0645\u0642\u0648\u062f \u0648\u0645\u0646\u0639 \u062a\u0644\u0641 \u0627\u0644\u0627\u0637\u0627\u0631\u0627\u062a \u0628\u0633\u0631\u0639\u0629","d":"\u0644\u062a\u0633\u0647\u064a\u0644 \u0639\u0645\u0644\u064a\u0629 \u0627\u0644\u0641\u0631\u0645\u0644\u0629","answer":"3"},{"id":1362,"question":"\u064a\u062c\u0628 \u062a\u0646\u0641\u064a\u0630 \u0639\u0645\u0644\u064a\u0629 \u0627\u0644\u062a\u0634\u062d\u064a\u0645 \u0648\u0627\u0644\u062a\u0632\u064a\u064a\u062a \u0644\u0644\u0627\u062f\u0648\u0627\u062a \u0627\u0644\u0632\u0631\u0627\u0639\u064a\u0629","a":"\u0639\u0646\u062f\u0645\u0627 \u064a\u0643\u0648\u0646 \u0627\u0644\u0645\u062d\u0631\u0643 \u0645\u0637\u0641\u0623 \u063a\u064a\u0631 \u0634\u063a\u0627\u0644","b":"\u0627\u062b\u0646\u0627\u0621 \u062f\u0648\u0631\u0627\u0646 \u0627\u0644\u0627\u062f\u0627\u0629 \u0627\u0644\u0632\u0631\u0627\u0639\u064a\u0629 \u0644\u0643\u064a \u064a\u062a\u0645\u0643\u0646 \u0627\u0644\u0632\u064a\u062a \u0645\u0646 \u0627\u0644\u062a\u062e\u0644\u062e\u0644 \u0644\u0643\u0644 \u0645\u0643\u0627\u0646","c":"\u0628\u0639\u062f \u0641\u0643 \u0627\u0644\u0627\u062f\u0648\u0627\u062a \u0627\u0644\u0632\u0631\u0627\u0639\u064a\u0629 \u0645\u0646 \u0627\u0644\u062a\u0631\u0627\u0643\u062a\u0648\u0631 \u0641\u0642\u0637","d":"\u0644\u0627 \u0627\u0647\u0645\u064a\u0629 \u0644\u0648\u0636\u0639 \u0627\u0644\u0627\u062f\u0648\u0627\u062a \u0627\u0644\u0632\u0631\u0627\u0639\u064a\u0629 \u0648\u0644\u062d\u0627\u0644\u0629 \u0627\u0644\u0645\u062d\u0631\u0643","answer":"1"},{"id":1363,"question":"\u064a\u062c\u0628 \u062a\u0646\u0641\u064a\u0630 \u0639\u0645\u0644\u064a\u0629 \u0627\u0644\u062a\u0634\u062d\u064a\u0645 \u0648\u0627\u0644\u062a\u0632\u064a\u064a\u062a \u0644\u0644\u0627\u062f\u0648\u0627\u062a \u0627\u0644\u0632\u0631\u0627\u0639\u064a\u0629","a":"\u0641\u0642\u0637 \u0628\u0639\u062f \u0641\u0643 \u0627\u0644\u0627\u062f\u0648\u0627\u062a \u0627\u0644\u0632\u0631\u0627\u0639\u064a\u0629 \u0645\u0646 \u0627\u0644\u062a\u0631\u0627\u0643\u062a\u0648\u0631","b":"\u0639\u0646\u062f\u0645\u0627 \u064a\u0639\u0645\u0644 \u0627\u0644\u0645\u062d\u0631\u0643 \u0648\u0630\u0644\u0643 \u0644\u062a\u0633\u0647\u064a\u0644 \u0639\u0645\u0644\u064a\u0629 \u0627\u0644\u062a\u0632\u064a\u064a\u062a","c":"\u0644\u0627 \u062a\u0648\u062c\u062f \u0627\u0647\u0645\u064a\u0629 \u0644\u0648\u0636\u0639 \u0627\u0644\u0627\u062f\u0648\u0627\u062a \u0627\u0644\u0632\u0631\u0627\u0639\u064a\u0629 \u0648\u0644\u062d\u0627\u0644\u0629 \u0627\u0644\u0645\u062d\u0631\u0643","d":"\u0639\u0646\u062f\u0645\u0627 \u064a\u0643\u0648\u0646 \u0627\u0644\u0645\u062d\u0631\u0643 \u0645\u0637\u0641\u0623","answer":"4"},{"id":1364,"question":"\u064a\u062c\u0628 \u062a\u0648\u0635\u064a\u0644 \u0627\u0644\u062a\u0631\u0627\u0643\u062a\u0648\u0631 \u0628\u0627\u0644\u0645\u0642\u0637\u0648\u0631\u0629 \u0628\u0648\u0627\u0633\u0637\u0629 \u0642\u0636\u064a\u0628 \u062c\u0631 \u0645\u0635\u0646\u0648\u0639 \u0645\u0646","a":"\u0627\u0644\u0641\u0648\u0644\u0627\u0630 \u0627\u0644\u0635\u0644\u0628","b":"\u0627\u0644\u0641\u0648\u0644\u0627\u0630 \u0627\u0644\u0646\u0627\u0639\u0645","c":"\u0644\u0648\u0644\u0628 \u0645\u0635\u0646\u0648\u0639 \u0645\u0646 \u0627\u0644\u0641\u0648\u0644\u0627\u0630","d":"\u0627\u0644\u062d\u062f\u064a\u062f","answer":"2"},{"id":1365,"question":"\u064a\u062c\u0628 \u062c\u0631 \u0645\u0642\u0637\u0648\u0631\u0629 \u0639\u0631\u0628\u0629 \u0645\u062c\u0631\u0648\u0631\u0629 \u0628\u0648\u0627\u0633\u0637\u0629","a":"\u0633\u0644\u0627\u0633\u0644 \u0627\u0645\u0627\u0646 \u0648\u062c\u0646\u0632\u064a\u0631 \u062c\u0631","b":"\u0639\u0627\u0645\u0648\u062f \u062c\u0631","c":"\u0642\u0636\u064a\u0628 \u062c\u0631 \u0641\u0642\u0637","d":"\u0633\u0644\u0627\u0633\u0644 \u0627\u0645\u0627\u0646 \u0648\u0642\u0636\u064a\u0628 \u062c\u0631","answer":"4"},{"id":1366,"question":"\u064a\u062c\u0628 \u0631\u0628\u0637 \u0627\u0644\u062a\u0631\u0627\u0643\u062a\u0648\u0631 \u0627\u0644\u0649 \u0627\u0644\u0645\u0642\u0637\u0648\u0631\u0629 (\u063a\u0631\u0628\u0629 \u0645\u062c\u0631\u0648\u0631\u0629 \u0628\u0648\u0627\u0633\u0637\u0629 \u0642\u0636\u064a\u0628 \u062c\u0631 \u0635\u0646\u0639 \u0645\u0646","a":"\u0644\u0648\u0644\u0628 \u0628\u0631\u063a\u064a \u0645\u0646 \u0627\u0644\u0641\u0648\u0644\u0627\u0630","b":"\u0644\u0627 \u064a\u0648\u062c\u062f \u0627\u0647\u0645\u064a\u0629 \u0645\u0646 \u0627\u064a \u0645\u0627\u062f\u0629 \u064a\u0635\u0646\u0639 \u0642\u0636\u064a\u0628 \u0627\u0644\u062c\u0631","c":"\u0627\u0644\u0641\u0648\u0644\u0627\u0630 \u0627\u0644\u0646\u0627\u0639\u0645","d":"\u0627\u0644\u0641\u0648\u0644\u0627\u062f \u0627\u0644\u0635\u0644\u0628 \u0627\u0644\u0635\u0639\u0628","answer":"3"},{"id":1367,"question":"\u064a\u062c\u0647\u0632 \u0627\u0644\u062a\u0631\u0627\u0643\u062a\u0648\u0631 \u0628\u0634\u0643\u0644 \u0639\u0627\u0645 \u0628\u062c\u0647\u0627\u0632 \u0641\u0631\u0627\u0645\u0644 \u064a\u0639\u0645\u0644 \u0639\u0644\u0649","a":"\u0627\u0644\u0639\u062c\u0644\u0627\u062a \u0627\u0644\u062e\u0644\u0641\u064a\u0629 \u0641\u0642\u0637","b":"\u0627\u0644\u0639\u062c\u0644\u0627\u062a \u0627\u0644\u0627\u0645\u0627\u0645\u064a\u0629 \u0641\u0642\u0637","c":"\u0627\u0644\u0639\u062c\u0644\u0627\u062a \u0627\u0644\u0627\u0645\u0627\u0645\u064a\u0629 \u0648\u0627\u0644\u062e\u0644\u0641\u064a\u0629 \u0645\u0639\u0627","d":"\u0639\u062c\u0644 \u0627\u0645\u0627\u0645\u064a \u0648\u0639\u062c\u0644 \u062e\u0644\u0641\u064a \u0628\u0634\u0643\u0644 \u0645\u062a\u0642\u0627\u0637\u0639","answer":"1"},{"id":1368,"question":"\u0644\u0645\u0627\u0630\u0627 \u064a\u0645\u0646\u0639 \u0641\u062a\u062d \u063a\u0637\u0627\u0621 \u0627\u0644\u0631\u0627\u062f\u064a\u0627\u062a\u0648\u0631 \u0639\u0646\u062f\u0645\u0627 \u064a\u0643\u0648\u0646 \u0627\u0644\u0645\u062d\u0631\u0643 \u0633\u0627\u062e\u0646\u0627\u064b :-","a":"\u0627\u062d\u062a\u0645\u0627\u0644 \u062e\u0631\u0648\u062c \u0627\u0644\u0645\u0627\u0621 \u0648\u063a\u0645\u0631 \u0634\u0645\u0639\u0627\u062a \u0627\u0644\u0627\u062d\u062a\u0631\u0627\u0642 ( \u0627\u0644\u0628\u0648\u062c\u064a\u0627\u062a ).","b":"\u062e\u0631\u0648\u062c \u0628\u062e\u0627\u0631 \u0645\u0627\u0621 \u0633\u0627\u062e\u0646 \u0642\u062f \u064a\u0634\u0648\u0647 \u062c\u0633\u0645 \u0627\u0644\u0625\u0646\u0633\u0627\u0646 .","c":"\u0627\u062d\u062a\u0645\u0627\u0644 \u0627\u0646\u0641\u062c\u0627\u0631 \u0627\u0644\u0645\u062d\u0631\u0643.","d":"\u0627\u062d\u062a\u0645\u0627\u0644 \u062f\u062e\u0648\u0644 \u0627\u0644\u0647\u0648\u0627\u0621 \u0625\u0644\u0649 \u0627\u0644\u062c\u064a\u0648\u0628 \u0627\u0644\u0645\u0627\u0626\u064a\u0629.","answer":"2"},{"id":1369,"question":"\u0639\u0646\u062f\u0645\u0627 \u064a\u0646\u062e\u0641\u0636 \u0645\u0633\u062a\u0648\u0649 \u0627\u0644\u0645\u062d\u0644\u0648\u0644 \u0641\u064a \u0627\u0644\u0628\u0637\u0627\u0631\u064a\u0629 \u0639\u0646 \u0627\u0644\u062d\u062f \u0627\u0644\u0644\u0627\u0632\u0645 \u0641\u0625\u0646\u0646\u0627 \u0646\u0642\u0648\u0645 \u0628\u0625\u0636\u0627\u0641\u0629\u061f","a":"\u0645\u062d\u0644\u0648\u0644 \u062d\u0627\u0645\u0636 \u0627\u0644\u0646\u064a\u062a\u0631\u064a\u0643(HNO3) \u0648\u0627\u0644\u0645\u0627\u0621 \u0627\u0644\u0645\u0642\u0637\u0631.","b":"\u0645\u0627\u0621 \u0645\u0642\u0637\u0631 \u0641\u0642\u0637.","c":"\u062d\u0627\u0645\u0636 \u0627\u0644\u0643\u0628\u0631\u064a\u062a\u064a\u0643 (H2SO4) \u0627\u0644\u0645\u0631\u0643\u0632 \u0641\u0642\u0637.","d":"\u0645\u062d\u0644\u0648\u0644 \u062d\u0627\u0645\u0636 \u0627\u0644\u0643\u0628\u0631\u064a\u062a\u064a\u0643 (H2SO4) \u0648\u0627\u0644\u0645\u0627\u0621 \u0627\u0644\u0645\u0642\u0637\u0631.","answer":"2"},{"id":1370,"question":"\u0625\u0630\u0627 \u062e\u0631\u062c \u0627\u0644\u062f\u062e\u0627\u0646 \u0628\u0644\u0648\u0646\u064d \u0627\u0632\u0631\u0642 \u0645\u0646 \u0627\u0644\u0627\u0643\u0632\u0648\u0633\u0633\u062a \u0641\u0630\u0644\u0643 \u064a\u062f\u0644 \u0639\u0644\u0649:-","a":"\u0627\u062d\u062a\u0631\u0627\u0642 \u0632\u064a\u062a \u0627\u0644\u0645\u062d\u0631\u0643.","b":"\u0627\u062d\u062a\u0631\u0627\u0642 \u0627\u0644\u0648\u0642\u0648\u062f \u0628\u0634\u0643\u0644\u064d \u063a\u064a\u0631 \u0643\u0627\u0645\u0644.","c":"\u062c\u0645\u064a\u0639 \u0627\u0644\u0625\u062c\u0627\u0628\u0627\u062a \u0635\u062d\u064a\u062d\u0629.","d":"\u0628\u062e\u0627\u0631 \u0645\u0627\u0621 \u062e\u0635\u0648\u0635\u0627\u064b \u0641\u064a \u0623\u0648\u0642\u0627\u062a \u0627\u0644\u0628\u0631\u062f.","answer":"1"}]