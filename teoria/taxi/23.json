u0647\u0644\u0649 \u064a\u062c\u0628 \u0639\u0644\u0649 \u0633\u0627\u0626\u0642 \u062a\u0627\u0643\u0633\u064a \u0625\u0639\u0637\u0627\u0621 \u0648\u0635\u0644 \u0645\u0642\u0627\u0628\u0644 \u0627\u062c\u0631\u0629 \u0627\u0644\u0633\u0641\u0631","a":"\u064a\u062c\u0628 \u0639\u0644\u0649 \u0633\u0627\u0626\u0642 \u0627\u0644\u062a\u0627\u0643\u0633\u064a \u0625\u0639\u0637\u0627\u0621 \u0648\u0635\u0644 \u0645\u0642\u0627\u0628\u0644 \u0627\u0644\u0623\u062c\u0631\u0629 \u062f\u0627\u0626\u0645\u0627","b":"\u0646\u0639\u0645 \u064a\u062c\u0628 \u0639\u0644\u064a\u0647 \u0625\u0639\u0637\u0627\u0621 \u0648\u0635\u0644 \u0627\u0630\u0627 \u0637\u0644\u0628 \u0627\u0644\u0631\u0627\u0643\u0628 \u0630\u0644\u0643","c":"\u0643\u0644\u0627 \u0644\u0627 \u064a\u062c\u0628 \u0639\u0644\u064a\u0647 \u0625\u0639\u0637\u0627\u0621 \u0648\u0635\u0644 \u0644\u0627\u0646 \u0647\u0630\u0627 \u0627\u0644\u0648\u0627\u062c\u0628 \u0645\u0641\u0631\u0648\u0636 \u0639\u0644\u0649 \u0633\u0627\u0626\u0642 \u0627\u0644\u0628\u0627\u0635 \u0641\u0642\u0637","d":"\u064a\u062c\u0628 \u0639\u0644\u0649 \u0627\u0644\u0633\u0627\u0626\u0642 \u0625\u0639\u0637\u0627\u0621 \u0648\u0635\u0644 \u0641\u064a \u0627\u0644\u0633\u0641\u0631\u064a\u0627\u062a \u0628\u064a\u0646 \u0627\u0644\u0645\u062f\u0646 \u0641\u0642\u0637","answer":"2"},{"id":2088,"question":"\u0627\u0644\u0627 \u064a\u0628\u062f\u0623 \u0633\u0627\u0626\u0642 \u0627\u0644\u0628\u0627\u0635 \u0628\u0639\u0645\u0644\u0647 \u0648\u0644\u0627 \u064a\u062c\u0648\u0632 \u0644\u0647 \u0627\u0635\u0639\u0627\u062f \u0627\u0644\u0631\u0643\u0627\u0628 \u0627\u0644\u0649 \u0627\u0644\u0628\u0627\u0635 \u0627\u0644\u0627 \u0627\u0630\u0627 ","a":"\u062a\u0623\u0643\u062f \u0645\u0646 \u0648\u062c\u0648\u062f \u0628\u0627\u0626\u0639 \u0627\u0644\u062a\u0630\u0627\u0643\u0631 \u062f\u0627\u062e\u0644 \u0627\u0644\u0628\u0627\u0635","b":"\u0642\u0627\u0645 \u0628\u0648\u0633\u0645 \u0627\u0644\u0645\u0642\u0627\u0639\u062f \u0641\u064a \u0627\u0644\u0628\u0627\u0635","c":"\u0642\u0627\u0645 \u0628\u062a\u0639\u0642\u064a\u0645 \u0627\u0644\u0628\u0627\u0635 \u062d\u0633\u0628 \u0623\u0648\u0627\u0645\u0631 \u0645\u0631\u0627\u0642\u0628 \u0627\u0644\u0633\u064a\u0631 \u0639\u0644\u0649 \u0627\u0644\u0637\u0631\u0642","d":"\u0627\u0647\u0645 \u0628\u0648\u0636\u0639 \u0627\u0644\u0627\u0641\u062a\u0627\u062a \u0631\u0642\u0645 \u0627\u0644\u062e\u0637 \u0645\u0646 \u0627\u0645\u0627\u0645 \u0648\u062e\u0644\u0641 \u0627\u0644\u0628\u0627\u0635","answer":"4"},{"id":2089,"question":"\u0628\u0623\u064a\u0629 \u0634\u0631\u0648\u0637 \u064a\u062c\u0628 \u0639\u0644\u0649 \u0633\u0627\u0626\u0642 \u0628\u0627\u0635 \u0639\u0645\u0648\u0645\u064a \u0627\u0646 \u064a\u0646\u0642\u0644 \u0645\u0639\u0647 \u0631\u0627\u0643\u0628\u0627 \u0627\u0630\u0627 \u0644\u0645 \u062a\u0648\u062c\u062f \u0645\u0639 \u062a\u0630\u0627\u0643\u0631 \u0645\u0644\u0627\u0626\u0645\u0629 ","a":"\u064a\u062c\u0628 \u0639\u0644\u0649 \u0627\u0644\u0633\u0627\u0626\u0642 \u0627\u0646 \u064a\u0646\u0642\u0644 \u0627\u0644\u0631\u0627\u0643\u0628 \u062d\u0633\u0628 \u062a\u0639\u0631\u0641\u0629 \u0623\u062c\u0648\u0631 \u0627\u0644\u0633\u0641\u0631 \u0641\u064a \u0647\u0630\u0627 \u0627\u0644\u062e\u0637 \u062d\u062a\u0649 \u0627\u0630\u0627 \u0644\u0645 \u064a\u0648\u062c\u062f \u0645\u0639 \u062a\u0630\u0627\u0643\u0631","b":"\u0644\u0627 \u064a\u0633\u062a\u0637\u064a\u0639 \u0627\u0644\u0633\u0627\u0626\u0642 \u0646\u0642\u0644 \u0631\u0627\u0643\u0628 \u0641\u064a \u0647\u0630\u0647 \u0627\u0644\u062d\u0627\u0644\u0629 \u0644\u0627\u0646\u0647 \u064a\u062e\u0627\u0644\u0641 \u0623\u0648\u0627\u0645\u0631 \u0645\u0631\u0627\u0642\u0628 \u0627\u0644\u0633\u064a\u0631 \u0639\u0644\u0649 \u0627\u0644\u0637\u0631\u0642","c":"\u064a\u0633\u062a\u0637\u064a\u0639 \u0644\u0644\u0633\u0627\u0626\u0642 \u0646\u0642\u0644 \u0627\u0644\u0631\u0627\u0643\u0628 \u0628\u062f\u0648\u0646 \u0627\u0646 \u064a\u062a\u0642\u0627\u0636\u0649 \u0645\u0646\u0647 \u0627\u062c\u0631\u0629 \u0627\u0644\u0633\u0641\u0631","d":"\u0630\u0644\u0643 \u064a\u0639\u062a\u0645\u062f \u0639\u0644\u0649 \u0631\u0623\u064a \u0627\u0644\u0633\u0627\u0626\u0642","answer":"1"},{"id":2090,"question":"\u0625\u0639\u0627\u062f\u0629 \u0636\u0628\u0637 \u0639\u062f\u0627\u062f \u0627\u0644\u062a\u0623\u0643\u0633\u064a","a":"\u0628\u0639\u062f \u0633\u0641\u0631 5000 \u0643\u062c\u0645","b":"\u0639\u0646\u062f\u0645\u0627 \u064a\u062a\u0645 \u062a\u063a\u064a\u064a\u0631 \u0625\u0637\u0627\u0631 \u0627\u0644\u0645\u0631\u0643\u0628\u0629 \u0628\u0645\u0642\u064a\u0627\u0633 \u064a\u062e\u062a\u0644\u0641 \u0648\u062e\u0644\u0627\u0644 48 \u0633\u0627\u0639\u0629 ","c":"\u0639\u0646\u062f\u0645\u0627 \u064a\u062a\u0645 \u062a\u063a\u064a\u064a\u0631 \u0645\u062d\u0631\u0643 \u0627\u0644\u0645\u0631\u0643\u0628\u0629 ","d":"\u0628\u0639\u062f \u062d\u062f\u0648\u062b \u0639\u0637\u0628 \u0641 \u0623\u062d\u062f \u0625\u0637\u0627\u0631\u0627\u062a \u0627\u0644\u0645\u0631\u0643\u0628\u0629 ","answer":"2"},{"id":2091,"question":"\u0647\u0644 \u064a\u062c\u0648\u0632 \u0644\u0633\u0627\u0626\u0642 \u0627\u0644\u062a\u0627\u0643\u0633\u064a \u0627\u0644\u0645\u062c\u0647\u0632 \u0628\u0639\u062f\u0627\u062f \u0627\u0646 \u064a\u0639\u0645\u0644 \u0628\u062f\u0648\u0646 \u062a\u0634\u063a\u064a\u0644 \u0627\u0644\u0639\u062f\u0627\u062f","a":"\u0644\u0627 \u064a\u062c\u0648\u0632 \u0644\u0633\u0627\u0626\u0642 \u0627\u0644\u062a\u0627\u0643\u0633\u064a \u0627\u0644\u0645\u062c\u0647\u0632 \u0628\u0639\u062f\u0627\u062f \u0627\u0646 \u064a\u0639\u0645\u0644 \u0628\u062f\u0648\u0646 \u062a\u0634\u063a\u064a\u0644 \u0627\u0644\u0639\u062f\u0627\u062f","b":"\u064a\u062c\u0648\u0632 \u0628\u0639\u062f \u062d\u0635\u0648\u0644\u0647 \u0639\u0644\u0649 \u062a\u0635\u0631\u064a\u062d \u062e\u0627\u0635 \u0645\u0646 \u0627\u0644\u0633\u0644\u0637\u0629 \u0627\u0644\u0645\u062e\u062a\u0635\u0629 ","c":"\u064a\u062c\u0648\u0632 \u0627\u0630\u0627 \u0643\u0627\u0646 \u0627\u0644\u0639\u062f\u0627\u062f \u0641 \u064a\u062d\u0627\u0644\u0629 \u0639\u0637\u0644","d":"\u064a\u062c\u0648\u0632 \u0641\u064a \u0637\u0631\u064a\u0642 \u062e\u0627\u0631\u062c \u0627\u0644\u0645\u062f\u0646 \u0641\u0642\u0637","answer":"1"},{"id":2092,"question":"\u064a\u062c\u0628 \u0627\u0646 \u064a\u0643\u0648\u0646 \u0627\u0644\u0628\u0627\u0635 \u0645\u0632\u0648\u062f","a":"\u0628\u0645\u0637\u0641\u0623\u062a\u064a\u0646 \u0644\u0644\u062d\u0631\u064a\u0642 \u0648\u0645\u0637\u0631\u0642\u062a\u064a\u0646","b":"\u064a\u0643\u062a\u0641\u064a \u0628\u0645\u0637\u0641\u0623\u0629 \u062d\u0631\u064a\u0642 \u0648\u0645\u0637\u0631\u0642\u062a\u064a\u0646","c":"\u064a\u0643\u062a\u0641\u064a \u0628\u0645\u0637\u0627\u0641\u0623\u0629 \u062d\u0631\u064a\u0642 \u0648\u0645\u0637\u0631\u0642\u0629 \u0648\u0627\u062d\u062f\u0629","d":"\u0627\u0630\u0627 \u0643\u0627\u0646\u062a \u0647\u0646\u0627\u0644\u0643 \u0645\u0637\u0641\u0623\u0629 \u062d\u0631\u064a\u0642 \u0641\u0644\u0627 \u062f\u0627\u0639\u064a \u0644\u0644\u0645\u0637\u0631\u0642\u0629 ","answer":"1"},{"id":2093,"question":"\u0627\u0644\u062a\u0641\u0633\u064a\u0631 \u0627\u0644\u0642\u0627\u0646\u0648\u0646\u064a \u0644\u0644\u0631\u0627\u0643\u0628","a":"\u0643\u0644 \u0634\u062e\u0635 \u064a\u0631\u064a\u062f \u0627\u0644\u0633\u0641\u0631 \u0648\u0644\u0647 \u0627\u0633\u062a\u0639\u062f\u0627\u062f \u0644\u062f\u0641\u0639 \u0627\u0644\u0623\u062c\u0631\u0629 ","b":"\u0643\u0644 \u0634\u062e\u0635 \u064a\u062d\u0645\u0644 \u0628\u0637\u0627\u0642\u0629 \u0647\u0648\u064a\u0629 ","c":"\u0643\u0644 \u0634\u062e\u0635 \u064a\u0632\u064a\u062f \u0639\u0645\u0631\u0647 \u0639\u0646 14 \u0639\u0627\u0645\u0627","d":"\u0627\u0644\u0634\u062e\u0635 \u0627\u0644\u0630\u064a \u064a\u0642\u0648\u0645 \u0628\u0625\u0633\u062a\u062c\u0626\u0627\u0631 \u0645\u0631\u0643\u0628\u0629 \u0627\u0644\u0623\u062c\u0631\u0629 ","answer":"1"},{"id":2094,"question":"\u0627\u0644\u0645\u0633\u0645\u0648\u062d \u0644\u0647 \u0628\u062a\u0631\u0643\u064a\u0628 \u0627\u0648 \u0636\u0628\u0637 \u0627\u0648 \u062a\u0635\u0644\u064a\u062d \u0639\u062f\u0627\u062f\u0627\u062a \u0627\u0644\u062a\u0627\u0643\u0633\u064a","a":"\u0635\u0627\u062d\u0628 \u0645\u0647\u0646\u0629 \u0645\u062e\u062a\u0635 \u0648\u064a\u062d\u0645\u0644 \u062a\u0631\u062e\u064a\u0635 \u0641\u064a \u062a\u0635\u0644\u064a\u062d \u0627\u0644\u0639\u062f\u0627\u062f\u0627\u062a","b":"\u0643\u0644 \u0648\u0631\u0634\u0629 \u0645\u0631\u062e\u0635\u0629 \u0644\u0643\u0647\u0631\u0628\u0627\u0621 \u0627\u0644\u0633\u064a\u0627\u0631\u0627\u062a","c":"\u0627\u0644\u0634\u0631\u0643\u0629 \u0627\u0644\u0645\u0646\u062a\u062c\u0629 \u0644\u0644\u0645\u0631\u0643\u0628\u0629 \u0641\u0642\u0637","d":"\u0635\u0627\u062d\u0628 \u0627\u0644\u0645\u0643\u0631\u0628\u0629 \u0627\u0648 \u0633\u0627\u0626\u0642\u0647\u0627","answer":"1"},{"id":2095,"question":"\u0645\u0627 \u0647\u0648 \u062c\u0647\u0627\u0632 \u0645\u0631\u0627\u0642\u0628\u0629 \u0627\u0644\u062a\u0648\u0627\u0632\u0646 (\u0627\u0644\u062b\u0628\u0627\u062a) \u0627\u0644\u0627\u0644\u0643\u062a\u0631\u0648\u0646\u064a \u0641\u064a \u0627\u0644\u0645\u0631\u0643\u0628\u0629 (\u0625\u064a-\u0625\u0633. \u0628\u064a) (E.S.P):","a":"\u062c\u0647\u0627\u0632 \u064a\u0645\u0646\u0639 \u0625\u0645\u0643\u0627\u0646\u064a\u0629 \u062d\u062f\u0648\u062b \u062e\u0644\u0644 \u0641\u064a \u062c\u0647\u0627\u0632 \u0641\u0631\u0627\u0645\u0644 \u0627\u0644\u0645\u0631\u0643\u0628\u0629.","b":"\u062c\u0647\u0627\u0632 \u064a\u0645\u0646\u0639 \u0625\u0645\u0643\u0627\u0646\u064a\u0629 \u062d\u062f\u0648\u062b \u062e\u0644\u0644 \u0641\u064a \u062c\u0647\u0627\u0632 \u0645\u0642\u0648\u062f \u0627\u0644\u0645\u0631\u0643\u0628\u0629.","c":"\u062c\u0647\u0627\u0632 \u064a\u0635\u062d\u062d \u0623\u062e\u0637\u0627\u0621 \u0627\u0644\u0633\u0627\u0626\u0642 \u0639\u0646\u062f \u0627\u0644\u0641\u0631\u0645\u0644\u0629 \u064a\u0645\u0646\u0639 \u0627\u0644\u062a\u0632\u062d\u0644\u0642 \u0648\u064a\u0633\u0627\u0639\u062f \u0639\u0644\u0649 \u0632\u064a\u0627\u062f\u0629 \u062a\u0648\u0627\u0632\u0646 \u0627\u0644\u0645\u0631\u0643\u0628\u0629.","d":"\u062c\u0647\u0627\u0632 \u064a\u0645\u0646\u0639 \u0625\u0642\u0641\u0627\u0644 \u0639\u062c\u0644\u0627\u062a \u0627\u0644\u0645\u0631\u0643\u0628\u0629 \u0639\u0646\u062f \u0627\u0644\u0641\u0631\u0645\u0644\u0629 \u0627\u0644\u0641\u062c\u0627\u0626\u064a\u0629\u060c \u0648\u0628\u0647\u0630\u0627 \u062a\u062a\u0645 \u0627\u0644\u0645\u062d\u0627\u0641\u0638\u0629 \u0639\u0644\u0649 \u0639\u0645\u0644 \u0645\u0642\u0648\u062f \u0627\u0644\u0645\u0631\u0643\u0628\u0629.","answer":"3"},{"id":2096,"question":"\u0623\u064a \u062c\u0632\u0621 \u0641\u064a \u0627\u0644\u0645\u0631\u0643\u0628\u0629 \u064a\u0642\u0644\u0644 \u0645\u0646 \u062a\u0623\u0631\u062c\u062d \u0627\u0644\u0631\u0623\u0633 \u0625\u0644\u0649 \u0627\u0644\u062e\u0644\u0641 \u0623\u062b\u0646\u0627\u0621 \u0627\u0644\u0627\u0635\u0637\u062f\u0627\u0645\u061f","a":"\u0633\u0627\u0646\u062f\u0629 \u0627\u0644\u0631\u0623\u0633 \u0641\u064a \u0645\u0642\u0627\u0639\u062f \u0627\u0644\u0645\u0631\u0643\u0628\u0629.","b":"\u0627\u0644\u0645\u0635\u062f (\u0627\u0644\u0637\u0645\u0628\u0648\u0646) \u0627\u0644\u062e\u0644\u0641\u064a.","c":"\u0648\u0633\u0627\u062f\u0629 \u0627\u0644\u0647\u0648\u0627\u0621.","d":"\u062d\u0632\u0627\u0645 \u0627\u0644\u0623\u0645\u0627\u0646.","answer":"1"},{"id":2097,"question":"\u0623\u064a\u0629 \u0645\u0644\u062d\u0642\u0627\u062a \u062a\u062e\u0641\u0641 \u0645\u0646 \u062e\u0637\u0648\u0631\u0629 \u0625\u0635\u0627\u0628\u0629 \u0627\u0644\u0631\u0643\u0627\u0628 \u0639\u0646\u062f \u0648\u0642\u0648\u0639 \u062d\u0627\u062f\u062b \u0637\u0631\u0642\u061f","a":"\u0623\u062d\u0632\u0645\u0629 \u0627\u0644\u0623\u0645\u0627\u0646 \u0648\u0633\u0627\u0646\u062f\u0627\u062a \u0627\u0644\u0631\u0623\u0633 \u0648\u0648\u0633\u0627\u0626\u062f \u0627\u0644\u0647\u0648\u0627\u0621.","b":"\u0645\u0635\u0627\u0628\u064a\u062d \u0627\u0644\u0636\u0628\u0627\u0628 \u0648\u0627\u0644\u0645\u0642\u0627\u0639\u062f \u0627\u0644\u0637\u0628\u064a\u0629.","c":"\u0645\u0627\u0633\u062d\u0627\u062a \u0627\u0644\u0632\u062c\u0627\u062c \u0627\u0644\u062e\u0644\u0641\u064a\u0629 \u0648\u0633\u0627\u0646\u062f\u0627\u062a \u0627\u0644\u0631\u0623\u0633.","d":"\u062f\u0639\u0633\u0627\u062a \u0627\u0644\u0641\u0631\u0627\u0645\u0644 \u0648\u0627\u0644\u0643\u0644\u0627\u062a\u0634.","answer":"1"},{"id":2098,"question":"\u0645\u0646 \u0627\u062c\u0644 \u0627\u0644\u0627\u0646\u062a\u0642\u0627\u0644 \u0628\u0627\u0644\u063a\u064a\u0627\u0631 \u0645\u0646 \u0648\u0636\u0639 \u0627\u0644\u0648\u0642\u0648\u0641 (P) \u0625\u0644\u0649 \u0648\u0636\u0639 \u0627\u0644\u0633\u064a\u0631 (R) \u0623\u0648 (D)\u060c \u0641\u064a \u0645\u0631\u0643\u0628\u0629 \u0630\u0627\u062a \u0635\u0646\u062f\u0648\u0642 \u063a\u064a\u0627\u0631\u0627\u062a \u0623\u0648\u062a\u0648\u0645\u0627\u062a\u0643\u064a\u0629 :","a":"\u064a\u062c\u0628 \u0627\u0644\u0636\u063a\u0637 \u0639\u0644\u0649 \u0645\u0641\u062a\u0627\u062d (\u0632\u0631) \u0627\u0644\u062a\u0634\u063a\u064a\u0644 \u062d\u064a\u062b \u064a\u062d\u062f\u062b \u0627\u0644\u062a\u0639\u0634\u064a\u0642 \u0628\u0634\u0643\u0644\u064d \u0623\u0648\u062a\u0648\u0645\u0627\u062a\u064a\u0643\u064a.","b":"\u064a\u062c\u0628 \u0627\u0644\u0636\u063a\u0637 \u0639\u0644\u0649 \u062f\u0639\u0633\u0629 \u0627\u0644\u0643\u0644\u0627\u062a\u0634 \u0648\u062a\u0639\u0634\u064a\u0642 \u0639\u0635\u0627 \u0627\u0644\u063a\u064a\u0627\u0631\u0627\u062a.","c":"\u064a\u062c\u0628 \u0627\u0644\u0636\u063a\u0637 \u0639\u0644\u0649 \u062f\u0639\u0633\u0629 \u0627\u0644\u0641\u0631\u0627\u0645\u0644 \u0648\u0639\u0644\u0649 \u0632\u0631 \u0627\u0644\u062a\u0634\u063a\u064a\u0644 \u0641\u064a \u0639\u0635\u0627 \u0627\u0644\u063a\u064a\u0627\u0631\u0627\u062a \u0648\u062a\u0639\u0634\u064a\u0642 \u0639\u0635\u0627 \u0627\u0644\u063a\u064a\u0627\u0631\u0627\u062a \u062d\u0633\u0628 \u0648\u0636\u0639 \u0627\u0644\u0633\u064a\u0631.","d":"\u064a\u062c\u0628 \u0627\u0644\u0636\u063a\u0637 \u0639\u0644\u0649 \u062f\u0639\u0633\u0629 \u0627\u0644\u0641\u0631\u0627\u0645\u0644 \u062d\u064a\u062b \u064a\u062d\u062f\u062b \u0627\u0644\u062a\u0639\u0634\u064a\u0642 \u0628\u0634\u0643\u0644\u064d \u0623\u0648\u062a\u0648\u0645\u0627\u062a\u064a\u0643\u064a.","answer":"3"},{"id":2099,"question":"\u0644\u0645\u0627\u0630\u0627 \u064a\u062c\u0628 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u0641\u0631\u0645\u0644\u0629 \u0627\u0644\u0648\u0642\u0648\u0641 (\u0627\u0644\u0647\u0627\u0646\u062f\u0628\u0631\u064a\u0643)-\u0639\u0646\u062f \u0627\u0644\u0648\u0642\u0648\u0641 \u0641\u0642\u0637\u061f","a":"\u0644\u0623\u0646 \u0639\u0645\u0631\u0647\u0627 \u0642\u0635\u064a\u0631 \u0648\u0644\u0627 \u064a\u062c\u0628 \u0627\u0633\u062a\u0646\u0641\u0627\u0630\u0647\u0627 \u0639\u0644\u0649 \u0627\u0644\u0641\u0631\u0645\u0644\u0629 \u0627\u0644\u0639\u0627\u062f\u064a\u0629.","b":"\u0644\u0627 \u064a\u062c\u0648\u0632 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u0641\u0631\u0645\u0644\u0629 \u0627\u0644\u0648\u0642\u0648\u0641 (\u0627\u0644\u0647\u0627\u0646\u062f\u0628\u0631\u064a\u0643) \u0643\u0641\u0631\u0645\u0644\u0629 \u0637\u0648\u0627\u0631\u0626.","c":"\u0644\u0623\u0646 \u0627\u0644\u064a\u062f \u0623\u0648 \u0627\u0644\u0631\u0627\u0641\u0639\u0629 \u0627\u0644\u0645\u0634\u063a\u0644\u0629 \u0644\u0647\u0627\u063a\u064a\u0631 \u0645\u0631\u064a\u062d\u0629 \u0641\u064a \u0627\u0644\u062a\u0634\u063a\u064a\u0644.","d":"\u0641\u0631\u0645\u0644\u0629 \u0627\u0644\u0648\u0642\u0648\u0641 (\u0627\u0644\u0647\u0627\u0646\u062f\u0628\u0631\u064a\u0643) \u0644\u0627 \u062a\u0639\u0645\u0644 \u0639\u0644\u0649 \u062c\u0645\u064a\u0639 \u0639\u062c\u0644\u0627\u062a \u0627\u0644\u0645\u0631\u0643\u0628\u0629 \u0648\u0642\u0648\u062a\u0647\u0627 \u0636\u0639\u064a\u0641\u0629 \u062c\u062f\u0627\u064b.","answer":"4"},{"id":2100,"question":"\u0645\u0627 \u0647\u064a \u0648\u0638\u064a\u0641\u0629 \u062c\u0647\u0627\u0632 \u0627\u0644-(\u0623\u064a. \u0628\u064a.\u0623\u0633) (ABS)\u061f","a":"\u0644\u0627\u0633\u062a\u0639\u0645\u0627\u0644\u0647 \u0643\u0641\u0631\u0645\u0644\u0629 \u0625\u0636\u0627\u0641\u064a\u0629 \u0648\u0642\u062a \u0627\u0644\u062e\u0637\u0631.","b":"\u0644\u062a\u0645\u0643\u064a\u0646 \u0627\u0644\u0633\u064a\u0637\u0631\u0629 \u0639\u0644\u0649 \u0627\u0644\u0645\u0631\u0643\u0628\u0629 \u0648\u0627\u0644\u0645\u0642\u0648\u062f \u0623\u062b\u0646\u0627\u0621 \u0627\u0644\u0641\u0631\u0645\u0644\u0629.","c":"\u0644\u0645\u0646\u0639 \u0627\u0647\u062a\u0631\u0627\u0621 \u0627\u0644\u0625\u0637\u0627\u0631\u0627\u062a.","d":"\u0644\u0645\u0646\u0639 \u0627\u0644\u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u0627\u0644\u0632\u0627\u0626\u062f \u0644\u0641\u0631\u0627\u0645\u0644 \u0627\u0644\u0645\u0631\u0643\u0628\u0629.","answer":"2"},{"id":2101,"question":"\u0644\u0645\u0627\u0630\u0627 \u062a\u0643\u0648\u0646 \u0623\u0647\u0645\u064a\u0629 \u0641\u064a \u0633\u0644\u0627\u0645\u0629 \u0645\u0635\u062f\u0627\u062a (\u0637\u0645\u0628\u0648\u0646\u0627\u062a) \u0627\u0644\u0645\u0631\u0643\u0628\u0629\u061f","a":"\u0645\u0646 \u0627\u062c\u0644 \u0627\u0644\u0645\u062d\u0627\u0641\u0638\u0629 \u0639\u0644\u0649 \u0634\u0643\u0644 \u0644\u0627\u0626\u0642 \u0644\u0644\u0645\u0631\u0643\u0628\u0629.","b":"\u0645\u0646 \u0627\u062c\u0644 \u0645\u0646\u0639 \u0627\u0644\u0636\u0631\u0631 \u0641\u064a \u0627\u0644\u062d\u0648\u0627\u062f\u062b \u0627\u0644\u0635\u0639\u0628\u0629.","c":"\u0645\u0646 \u0627\u062c\u0644 \u062a\u0642\u0644\u064a\u0644 \u0645\u062f\u0649 \u0627\u0644\u0636\u0631\u0631 \u0644\u0644\u0645\u0631\u0643\u0628\u0629 \u0648\u0644\u0631\u0643\u0627\u0628\u0647\u0627 \u0639\u0646\u062f \u0648\u0642\u0648\u0639 \u062d\u0627\u062f\u062b.","d":"\u0645\u0646 \u0627\u062c\u0644 \u0627\u0644\u0627\u0645\u062a\u0646\u0627\u0639 \u0639\u0646 \u062d\u0627\u062f\u062b \u0628\u0633\u064a\u0637 \u0646\u0633\u0628\u064a\u0627\u064b.","answer":"3"},{"id":2102,"question":"\u0645\u0627 \u0647\u064a \u0648\u0638\u064a\u0641\u0629 \u062c\u0647\u0627\u0632 \u0627\u0644-(\u0623\u064a.\u0628\u064a.\u0623\u0633) (ABS)\u061f","a":"\u0627\u0644\u0645\u0633\u0627\u0639\u062f\u0629 \u0639\u0644\u0649 \u0641\u0631\u0645\u0644\u0629 \u0633\u0644\u0633\u0629 \u0648\u0623\u0643\u062b\u0631 \u0627\u0639\u062a\u062f\u0627\u0644\u0627\u064b \u0639\u0644\u0649 \u0627\u0644\u0634\u0627\u0631\u0639 \u0627\u0644\u0631\u0637\u0628 (\u0627\u0644\u0645\u0628\u0644\u0648\u0644).","b":"\u0627\u0644\u0645\u062d\u0627\u0641\u0638\u0629 \u0639\u0644\u0649 \u0642\u062f\u0631\u0629 \u062a\u0648\u062c\u064a\u0647 \u0627\u0644\u0645\u0642\u0648\u062f \u0648\u0627\u0644\u0645\u0646\u0627\u0648\u0631\u0629 \u0628\u0627\u0644\u0645\u0631\u0643\u0628\u0629 \u0623\u062b\u0646\u0627\u0621 \u0641\u0631\u0645\u0644\u0629 \u0627\u0644\u0637\u0648\u0627\u0631\u0626 \u0623\u064a\u0636\u0627\u064b.","c":"\u062a\u0642\u0635\u064a\u0631 \u0639\u0645\u0631 \u0627\u0644\u0625\u0637\u0627\u0631\u0627\u062a.","d":"\u0627\u0644\u0645\u0633\u0627\u0639\u062f\u0629 \u0639\u0644\u0649 \u0641\u0631\u0645\u0644\u0629 \u0633\u0644\u0633\u0629 \u0648\u0623\u0643\u062b\u0631 \u0627\u0639\u062a\u062f\u0627\u0644\u0627\u064b \u0639\u0644\u0649 \u0627\u0644\u0634\u0627\u0631\u0639 \u0627\u0644\u062c\u0627\u0641.","answer":"2"},{"id":2103,"question":"\u0645\u0627 \u0647\u064a \u0648\u0638\u064a\u0641\u0629 \u062c\u0647\u0627\u0632 \u0645\u0631\u0627\u0642\u0628\u0629 \u0627\u0644\u062b\u0628\u0627\u062a \u0627\u0644\u0627\u0644\u0643\u062a\u0631\u0648\u0646\u064a \u0641\u064a \u0627\u0644\u0645\u0631\u0643\u0628\u0629\u2013(\u0625\u064a.\u0625\u0633.\u0628\u064a) (E.S.P)\u061f","a":"\u0627\u0644\u062a\u0642\u0644\u064a\u0644 \u0645\u0646 \u0643\u0645\u064a\u0629 \u0627\u0644\u063a\u0627\u0632\u0627\u062a \u0627\u0644\u0645\u062d\u062a\u0631\u0642\u0629 \u0648\u0627\u0644\u0645\u0646\u0628\u0639\u062b\u0629 \u0645\u0646 \u0627\u0644\u0645\u062d\u0631\u0643.","b":"\u0645\u0646\u0639 \u0625\u0642\u0641\u0627\u0644 \u0627\u0644\u0639\u062c\u0644\u0627\u062a.","c":"\u062c\u0647\u0627\u0632 \u064a\u0642\u0644\u0644 \u0645\u0646 \u062e\u0637\u0631 \u062a\u0632\u062d\u0644\u0642 \u0627\u0644\u0645\u0631\u0643\u0628\u0629.","d":"\u0627\u0644\u0633\u064a\u0637\u0631\u0629 \u0639\u0644\u0649 \u0627\u0633\u062a\u0647\u0644\u0627\u0643 \u0648\u0642\u0648\u062f \u0627\u0644\u0645\u062d\u0631\u0643.","answer":"3"},{"id":2104,"question":"\u0643\u064a\u0641 \u064a\u0639\u0645\u0644 \u062c\u0647\u0627\u0632 \u0645\u0631\u0627\u0642\u0628\u0629 \u0627\u0644\u062b\u0628\u0627\u062a \u0627\u0644\u0627\u0644\u0643\u062a\u0631\u0648\u0646\u064a (\u0625\u064a.\u0625\u0633.\u0628\u064a) (E.S.P) \u0641\u064a \u0627\u0644\u062d\u0627\u0644\u0627\u062a \u0627\u0644\u062e\u0637\u0631\u0629 \u0644\u0644\u0633\u064a\u0627\u0642\u0629\u061f","a":"\u064a\u064f\u0634\u063a\u0644 \u0627\u0644\u0641\u0631\u0627\u0645\u0644 \u0639\u0644\u0649 \u0643\u0644 \u0639\u062c\u0644 \u0639\u0644\u0649 \u0627\u0646\u0641\u0631\u0627\u062f \u0648\u064a\u062a\u062d\u0643\u0645 \u0641\u064a \u0642\u0648\u0629 \u0627\u0644\u0645\u062d\u0631\u0643.","b":"\u064a\u062a\u062d\u0643\u0645 \u0641\u064a \u0639\u062c\u0644\u0629 \u0627\u0644\u0645\u0642\u0648\u062f.","c":"\u064a\u062a\u062d\u0643\u0645 \u0641\u064a \u0642\u0648\u0629 \u0627\u0644\u0645\u062d\u0631\u0643 \u0641\u0642\u0637.","d":"\u064a\u0641\u062d\u0635 \u0636\u063a\u0637 \u0627\u0644\u0647\u0648\u0627\u0621 \u0641\u064a \u0627\u0644\u0625\u0637\u0627\u0631\u0627\u062a \u0648\u064a\u064f\u0634\u063a\u0644 \u0641\u0631\u0627\u0645\u0644 \u0627\u0644\u064a\u062f \u0645\u0646 \u0627\u062c\u0644 \u062a\u062e\u0641\u064a\u0641 \u0633\u0631\u0639\u0629 \u0627\u0644\u0645\u0631\u0643\u0628\u0629.","answer":"1"},{"id":2105,"question":"\u0623\u064a \u062c\u0647\u0627\u0632 \u064a\u0633\u0627\u0639\u062f \u0641\u064a \u062d\u0641\u0638 \u062a\u0648\u0627\u0632\u0646 \u0627\u0644\u0645\u0631\u0643\u0628\u0629 \u0639\u0646\u062f \u0627\u0644\u062a\u0632\u062d\u0644\u0642 \u0648\u062e\u0637\u0631 \u0627\u0644\u0627\u0646\u0642\u0644\u0627\u0628\u061f","a":"\u062c\u0647\u0627\u0632 \u0645\u0631\u0627\u0642\u0628\u0629 \u0627\u0644\u062b\u0628\u0627\u062a (\u0625\u064a.\u0625\u0633.\u0628\u064a) (E.S.P)","b":"\u062c\u0647\u0627\u0632 \u0625\u0639\u0637\u0627\u0621 \u0627\u0644\u0625\u0634\u0627\u0631\u0629.","c":"\u062c\u0647\u0627\u0632 \u0645\u0631\u0627\u0642\u0628\u0629 \u0627\u0644\u0633\u0631\u0639\u0629.","d":"\u062c\u0647\u0627\u0632 \u0645\u0631\u0627\u0642\u0628\u0629 \u0627\u0633\u062a\u0647\u0644\u0627\u0643 \u0627\u0644\u0648\u0642\u0648\u062f \u0641\u064a \u0627\u0644\u0645\u0631\u0643\u0628\u0629.","answer":"1"}]